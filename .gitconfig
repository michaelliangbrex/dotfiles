[user]
	name = Michael Liang
	mail = lliang@brex.com
[merge]
    tool = vscode
[mergetool "vscode"]
    cmd = code --wait $MERGED
[diff]
      tool = vscode
[difftool "vscode"]
      cmd = code --wait --diff $LOCAL $REMOTE
[url "git@github.com:"]
     insteadOf = https://github.com/
[push]
	default = simple
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[core]
	editor = vim
